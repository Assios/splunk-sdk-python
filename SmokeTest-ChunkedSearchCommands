splunk search '| generatehello count=1000 | outputcsv hello' \
    -app chunked_searchcommands -auth admin:changeme -maxout 0 -output csv

splunk search '| inputcsv hello | echo' \
    -app chunked_searchcommands -auth admin:changeme -maxout 0 -output csv

splunk search '| inputlookup tweets | countmatches fieldname=word_count pattern="\\w+" text | outputcsv word_counts' \
    -app chunked_searchcommands -auth admin:changeme -maxout 0 -output csv

splunk search '| inputcsv word_counts | sum total=total_word_count word_count' \
    -app chunked_searchcommands -auth admin:changeme -maxout 0 -output csv
